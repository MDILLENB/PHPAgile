    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/var-dumper": "*",        
        "phing/phing": "2.*",
        "squizlabs/php_codesniffer": "*",
        "phpcheckstyle/phpcheckstyle": "*",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd": "*",
        "pdepend/pdepend": "*",
        "phploc/phploc": "*",
        "tedivm/jshrink": "*",
        "pear/versioncontrol_git": "*",
        "pear/archive_tar": "*",
        "apigen/apigen": "*",
        "roave/better-reflection": "*",
        "umpirsky/twig-gettext-extractor": "*"
    },
    "autoload": {
        "psr-4": {           
