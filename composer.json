    "require": {
        "php": ">=7.0",
        "psr/container": "*", 
        "slim/slim": "*",        
        "slim/twig-view": "*",
        "twig/twig": "*",
        "twig/extensions": "*",
        "doctrine/dbal": "*",
        "doctrine/orm": "*",
        "monolog/monolog": "*",        
        "projek-xyz/slim-monolog": "*",   
        "davidepastore/slim-validation": "*",  
        "boronczyk/localization-middleware": "*",
        "guzzlehttp/guzzle": "*",            
        "pear/versioncontrol_git": "*"         
