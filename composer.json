            "Config\\": "src/Config/",
            "Presentation\\": "src/Presentation/",
            "Resource\\": "src/Resource/", 
            "Support\\": "src/Support/",    
            "Test\\": "test/", 
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {           
            "Business\\": "src/Business/",
            "Config\\": "src/Config/",
            "Presentation\\": "src/Presentation/",
            "Resource\\": "src/Resource/", 
            "Support\\": "src/Support/", 
            "Test\\": "test/", 
            "": "src/"
        }
    }
}
